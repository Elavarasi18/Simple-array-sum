#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
    int i=0,n,sum=0;
    scanf("%d",&n);
     int arr[n];
 for(int i=0;i<n;i++)
   scanf("%d",&arr[i]);
     
    for(int i=0;i<n;i++)
    {
       
            sum=sum+arr[i];
        
    }
    printf("%d\n",sum);
    return 0;
    
}

   
